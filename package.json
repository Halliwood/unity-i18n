{
  "name": "unity-i18n",
  "version": "1.0.98",
  "description": "i18n tool for unity game project",
  "main": "bin/index.js",
  "scripts": {
    "maintain": "node bin/maintain.js",
    "test:df:s": "node bin/index.js -s \"D:/works/df/trunk/project/\" -o \"D:/works/df/trunk/project/tools/i18n/dictionary\" -d \"unity_soft\" -S --soft-replace --langs TW,EN -x prepend --pretty --strict --lockfile buildlock",
    "test:dp:s": "node bin/index.js -s \"D:/works/dp/trunk/project/\" -o \"D:/works/dp/trunk/project/tools/i18n/dictionary\" -d \"unity_soft\" -S --soft-replace --langs TW,EN -x prepend --pretty --strict --lockfile buildlock",
    "test:dp:r": "node bin/index.js -s \"D:/works/dp/trunk/project/\" -o \"D:/works/dp/trunk/project/tools/i18n/dictionary\" -d \"unity_soft\" -R --soft-replace --langs TW,EN -x prepend --pretty --strict --lockfile buildlock",
    "test:dldltw": "node bin/index.js -s \"G:/dldltw/trunk/\" -o \"G:/dldltw/trunk/tools/i18n/dictionary/tw\" -d \"unity\" -R",
    "test:laya": "node bin/index.js -s \"G:/qhgame/trunk/project/\" -o \"G:/qhgame/trunk/project/tools/i18n/dictionary/tw\" -d \"laya\" --task-replacer \"$workspace=F:/layai18nwork/QHGame\" -S",
    "build": "tsc",
    "release": "npm config set registry=https://registry.npmjs.org && npm version patch && npm publish",
    "setproxy": "git config --global http.https://github.com.proxy socks5://127.0.0.1:10808 && git config --global https.https://github.com.proxy socks5://127.0.0.1:10808",
    "unsetproxy": "git config --global --unset http.proxy",
    "unsetproxy2": "git config --global --unset https.proxy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Halliwood/unity-i18n.git"
  },
  "keywords": [
    "i18n",
    "unity",
    "laya",
    "localization",
    "chinese",
    "translation"
  ],
  "author": "teppei",
  "bin": "bin/unity-i18n",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Halliwood/unity-i18n/issues"
  },
  "homepage": "https://github.com/Halliwood/unity-i18n#readme",
  "dependencies": {
    "commander": "^7.2.0",
    "fs-extra": "^10.1.0",
    "md5": "^2.3.0",
    "proper-lockfile": "^4.1.2",
    "sleep-promise": "^9.1.0",
    "xlsx": "^0.16.6",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/md5": "^2.2.0",
    "@types/node": "^14.6.3",
    "@types/proper-lockfile": "^4.1.2",
    "@types/typescript": "^2.0.0",
    "@types/xlsx": "0.0.36",
    "@types/yaml": "^1.9.7"
  }
}
