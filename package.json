{
  "name": "unity-i18n",
  "version": "1.0.40",
  "description": "i18n tool for unity game project",
  "main": "bin/index.js",
  "scripts": {
    "test": "tsc && node bin/index.js -s \"G:/dldl_kr/trunk/\" -o \"G:/dldl_kr/trunk/tools/i18n/dictionary/tw\" -dS",
    "build": "tsc",
    "release": "npm config set registry=http://registry.npmjs.org && npm version patch && npm publish",
    "setproxy": "git config --global http.https://github.com.proxy socks5://127.0.0.1:1080 && git config --global https.https://github.com.proxy socks5://127.0.0.1:1080",
    "unsetproxy": "git config --global --unset http.proxy",
    "unsetproxy2": "git config --global --unset https.proxy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Halliwood/unity-i18n.git"
  },
  "keywords": [
    "i18n",
    "unity",
    "localization"
  ],
  "author": "teppei",
  "bin": "bin/unity-i18n",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Halliwood/unity-i18n/issues"
  },
  "homepage": "https://github.com/Halliwood/unity-i18n#readme",
  "dependencies": {
    "commander": "^6.1.0",
    "md5": "^2.3.0",
    "xlsx": "^0.16.6",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@types/md5": "^2.2.0",
    "@types/node": "^14.6.3",
    "@types/typescript": "^2.0.0",
    "@types/xlsx": "0.0.36",
    "@types/yaml": "^1.9.7"
  }
}
